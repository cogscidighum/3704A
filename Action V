<!DOCTYPE html>
<html lang="en-us">
  <head>
    <meta charset="utf-8">
    <title>CelSci</title>
  </head>

  <body>
    <script>
        const celscilist = {
          'Jim Davies': { 'info': { 'views': 600 } },
          'Jim Davies': { 'info': { 'views': 400 } },
        };
        
        let sumOfViews = 0;
        let count = 0;

        for (const key in celscilist) {
          if (celscilist.hasOwnProperty(key)) {
            const views = celscilist[key].info.views;
            sumOfViews += views;
            count++;
          }
        }

        const averageViews = sumOfViews / count;
        
        class Celsci {
          static iscelsci(x) {
            let y = "false";
            if (x > averageViews) {
              y = "true";
            }
            return y;
          }
        }
        
        function checkcelsci(x) {
          const result = Celsci.iscelsci(celscilist[x]['info']['views']);
          return result;
        }
        
        document.write(checkcelsci('dd'));
        
    </script>
  </body>
</html>
